// RUN: marco --omc-bypass --model=Asin --end-time=1 -o simulation_asin %s
// RUN: ./simulation_asin | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;0.000000000000
// CHECK-NEXT: 0.100000000000;0.000000000000
// CHECK-NEXT: 0.200000000000;0.100000000000
// CHECK-NEXT: 0.300000000000;0.189983257884
// CHECK-NEXT: 0.400000000000;0.270868748503
// CHECK-NEXT: 0.500000000000;0.343439208119
// CHECK-NEXT: 0.600000000000;0.408381567776
// CHECK-NEXT: 0.700000000000;0.466313533973
// CHECK-NEXT: 0.800000000000;0.517801645107
// CHECK-NEXT: 0.900000000000;0.563373717512
// CHECK-NEXT: 1.000000000000;0.603527362876

model Asin
    Real x;
equation
    der(x) = 1 - asin(x);
end Asin;
