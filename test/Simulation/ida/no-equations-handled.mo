// RUN: marco --omc-bypass --model=NoEquationsForIDA --end-time=1 --time-step=0.1 --solver=ida -o %basename_t %s
// RUN: ./%basename_t | FileCheck %s --check-prefix="CHECK-NO-EQUIDISTANT"

// CHECK-NO-EQUIDISTANT: time;x
// CHECK-NO-EQUIDISTANT-NEXT: 0.000000000000;2.000000000000
// CHECK-NO-EQUIDISTANT-NEXT: 1.000000000000;2.000000000000

model NoEquationsForIDA
	Real x;
equation
	x = 2;
end NoEquationsForIDA;
