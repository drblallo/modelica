// RUN: marco --omc-bypass --model=Tanh --end-time=1 -o simulation_tanh %s
// RUN: ./simulation_tanh | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;0.000000000000
// CHECK-NEXT: 0.100000000000;0.000000000000
// CHECK-NEXT: 0.200000000000;0.100000000000
// CHECK-NEXT: 0.300000000000;0.190033200538
// CHECK-NEXT: 0.400000000000;0.271255376944
// CHECK-NEXT: 0.500000000000;0.344776119036
// CHECK-NEXT: 0.600000000000;0.411602656540
// CHECK-NEXT: 0.700000000000;0.472619393503
// CHECK-NEXT: 0.800000000000;0.528588063038
// CHECK-NEXT: 0.900000000000;0.580157957118
// CHECK-NEXT: 1.000000000000;0.627879934445

model Tanh
    Real x;
equation
    der(x) = 1 - tanh(x);
end Tanh;
