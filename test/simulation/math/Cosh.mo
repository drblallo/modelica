// RUN: marco --omc-bypass --model=Cosh --end-time=1 -o simulation_cosh %s
// RUN: ./simulation_cosh | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;0.000000000000
// CHECK-NEXT: 0.100000000000;0.000000000000
// CHECK-NEXT: 0.200000000000;0.000000000000
// CHECK-NEXT: 0.300000000000;0.000000000000
// CHECK-NEXT: 0.400000000000;0.000000000000
// CHECK-NEXT: 0.500000000000;0.000000000000
// CHECK-NEXT: 0.600000000000;0.000000000000
// CHECK-NEXT: 0.700000000000;0.000000000000
// CHECK-NEXT: 0.800000000000;0.000000000000
// CHECK-NEXT: 0.900000000000;0.000000000000
// CHECK-NEXT: 1.000000000000;0.000000000000

model Cosh
    Real x;
equation
    der(x) = 1 - cosh(x);
end Cosh;
