// RUN: marco --omc-bypass --model=Sqrt --end-time=1 -o simulation %s
// RUN: ./simulation | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;0.000000000000
// CHECK-NEXT: 0.100000000000;0.000000000000
// CHECK-NEXT: 0.200000000000;0.100000000000
// CHECK-NEXT: 0.300000000000;0.168377223398
// CHECK-NEXT: 0.400000000000;0.227343429594
// CHECK-NEXT: 0.500000000000;0.279662885386
// CHECK-NEXT: 0.600000000000;0.326779723096
// CHECK-NEXT: 0.700000000000;0.369615072909
// CHECK-NEXT: 0.800000000000;0.408819096652
// CHECK-NEXT: 0.900000000000;0.444880133831
// CHECK-NEXT: 1.000000000000;0.478180798465

model Sqrt
    Real x;
equation
    der(x) = 1 - sqrt(x);
end Sqrt;
