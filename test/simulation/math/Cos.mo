// RUN: marco --omc-bypass --model=Cos --end-time=1 -o simulation_cos %s
// RUN: ./simulation_cos | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;0.000000000000
// CHECK-NEXT: 0.100000000000;0.000000000000
// CHECK-NEXT: 0.200000000000;0.100000000000
// CHECK-NEXT: 0.300000000000;0.199500416528
// CHECK-NEXT: 0.400000000000;0.297516987272
// CHECK-NEXT: 0.500000000000;0.393123719654
// CHECK-NEXT: 0.600000000000;0.485495414385
// CHECK-NEXT: 0.700000000000;0.573939801807
// CHECK-NEXT: 0.800000000000;0.657916642141
// CHECK-NEXT: 0.900000000000;0.737043427937
// CHECK-NEXT: 1.000000000000;0.811089318840

model Cos
    Real x;
equation
    der(x) = cos(x);
end Cos;
