// RUN: marco --omc-bypass --model=ImplicitKepler --end-time=1 --time-step=0.1 --solver=ida --ida-equidistant-time-grid -o %basename_t %s
// RUN: ./%basename_t | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.100000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.200000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.300000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.400000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.500000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.600000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.700000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.800000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 0.900000000000;3.657695464140;3.657695464140
// CHECK-NEXT: 1.000000000000;3.657695464140;3.657695464140

model ImplicitKepler
	Real[2] x(start = 3.6);
equation
	for i in 1:2 loop
		5.0 = x[i] - 2.72 * sin(x[i]);
	end for;
end ImplicitKepler;
