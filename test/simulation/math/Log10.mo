// RUN: marco --omc-bypass --model=Log10 --end-time=1 -o simulation_log10 %s
// RUN: ./simulation_log10 | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;0.000000000000
// CHECK-NEXT: 0.100000000000;0.000000000000
// CHECK-NEXT: 0.200000000000;0.200000000000
// CHECK-NEXT: 0.300000000000;0.400086772153
// CHECK-NEXT: 0.400000000000;0.600260180969
// CHECK-NEXT: 0.500000000000;0.800520091362
// CHECK-NEXT: 0.600000000000;1.000866368652
// CHECK-NEXT: 0.700000000000;1.201298878563
// CHECK-NEXT: 0.800000000000;1.401817487216
// CHECK-NEXT: 0.900000000000;1.602422061136
// CHECK-NEXT: 1.000000000000;1.803112467241

model Log10
    Real x;
equation
    der(x) = log10(100 + x);
end Log10;
