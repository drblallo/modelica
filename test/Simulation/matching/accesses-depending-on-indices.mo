// RUN: marco --omc-bypass --model=AccessesDependingOnIndices --solver=forward-euler --end-time=1 --time-step=0.1 -o %basename_t %s
// RUN: ./%basename_t --precision=6 | FileCheck %s

// CHECK: time;x[1,1];x[1,2];x[1,3];x[1,4];x[2,1];x[2,2];x[2,3];x[2,4];x[3,1];x[3,2];x[3,3];x[3,4]
// CHECK-NEXT: 0.000000;0.000000;0.000000;0.000000;0.000000;0.000000;0.000000;0.000000;0.000000;0.000000;0.000000;0.000000;0.000000
// CHECK-NEXT: 0.100000;0.400000;0.400000;0.400000;0.400000;0.400000;0.400000;0.400000;0.400000;0.400000;0.400000;0.400000;0.400000
// CHECK-NEXT: 0.200000;0.800000;0.800000;0.800000;0.800000;0.800000;0.800000;0.800000;0.800000;0.800000;0.800000;0.800000;0.800000
// CHECK-NEXT: 0.300000;1.200000;1.200000;1.200000;1.200000;1.200000;1.200000;1.200000;1.200000;1.200000;1.200000;1.200000;1.200000
// CHECK-NEXT: 0.400000;1.600000;1.600000;1.600000;1.600000;1.600000;1.600000;1.600000;1.600000;1.600000;1.600000;1.600000;1.600000
// CHECK-NEXT: 0.500000;2.000000;2.000000;2.000000;2.000000;2.000000;2.000000;2.000000;2.000000;2.000000;2.000000;2.000000;2.000000
// CHECK-NEXT: 0.600000;2.400000;2.400000;2.400000;2.400000;2.400000;2.400000;2.400000;2.400000;2.400000;2.400000;2.400000;2.400000
// CHECK-NEXT: 0.700000;2.800000;2.800000;2.800000;2.800000;2.800000;2.800000;2.800000;2.800000;2.800000;2.800000;2.800000;2.800000
// CHECK-NEXT: 0.800000;3.200000;3.200000;3.200000;3.200000;3.200000;3.200000;3.200000;3.200000;3.200000;3.200000;3.200000;3.200000
// CHECK-NEXT: 0.900000;3.600000;3.600000;3.600000;3.600000;3.600000;3.600000;3.600000;3.600000;3.600000;3.600000;3.600000;3.600000
// CHECK-NEXT: 1.000000;4.000000;4.000000;4.000000;4.000000;4.000000;4.000000;4.000000;4.000000;4.000000;4.000000;4.000000;4.000000

model AccessesDependingOnIndices
	Real[3, 4] x;
equation
	for i in 1:3 loop
		for j in 1:4 loop
			der(x[i, j]) = 2 * der(x[2, 2]) - 4;
		end for;
	end for;
end AccessesDependingOnIndices;
