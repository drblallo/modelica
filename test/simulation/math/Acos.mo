// RUN: marco --omc-bypass --model=Acos --end-time=1 -o simulation_acos %s
// RUN: ./simulation_acos | FileCheck %s

// CHECK: time;x
// CHECK-NEXT: 0.000000000000;0.000000000000
// CHECK-NEXT: 0.100000000000;0.000000000000
// CHECK-NEXT: 0.200000000000;-0.057079632679
// CHECK-NEXT: 0.300000000000;-0.119870332684
// CHECK-NEXT: 0.400000000000;-0.188965892594
// CHECK-NEXT: 0.500000000000;-0.265056421228
// CHECK-NEXT: 0.600000000000;-0.348962297504
// CHECK-NEXT: 0.700000000000;-0.441688286517
// CHECK-NEXT: 0.800000000000;-0.544515878968
// CHECK-NEXT: 0.900000000000;-0.659176692988
// CHECK-NEXT: 1.000000000000;-0.788228665096

model Acos
    Real x;
equation
    der(x) = 1 - acos(x);
end Acos;
