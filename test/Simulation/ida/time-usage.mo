// CHECK-NEXT: 0.300000000000;0.045000112203
// CHECK-NEXT: 0.400000000000;0.080000116387
// CHECK-NEXT: 0.500000000000;0.125000120929
// CHECK-NEXT: 0.600000000000;0.180000124974
// CHECK-NEXT: 0.700000000000;0.245000128191
// CHECK-NEXT: 0.800000000000;0.320000130580
// CHECK-NEXT: 0.900000000000;0.405000131998
// CHECK-NEXT: 1.000000000000;0.500000132489

model TimeUsage
	Real x;
equation
	der(x) = time;
end TimeUsage;
